{
  "expo": {
    "name": "Gbst",
    "slug": "gbst",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon3.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.inioluwajeremiah.gbst"
    },
    "web": {
      "favicon": "./assets/icon3.png"
    },
    "extra": {
      "eas": {
        "projectId": "b489a8d3-3971-4d18-8e6b-e22472b39703"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}

   
  


